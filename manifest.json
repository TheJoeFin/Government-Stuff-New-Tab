{
    "manifest_version": 3,
    "name": "Milwaukee Government Representatives & Officials",
    "version": "2.1.0",
    "description": "New tab page with comprehensive Milwaukee area government representatives, local officials, and favorite sites. Find your alderperson and all government officials easily.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://milwaukee-alder-apim.azure-api.net/*",
        "https://www.googleapis.com/*",
        "https://api.propublica.org/*",
        "https://api.bigdatacloud.net/*",
        "https://www.google.com/s2/favicons*",
        "https://icons.duckduckgo.com/*",
        "https://*.com/favicon.ico",
        "https://*.org/favicon.ico",
        "https://*.net/favicon.ico"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}